[submodule "deps/SDL2"]
	path = deps/SDL2
	url = https://github.com/libsdl-org/SDL.git
	branch = SDL2
	update = checkout
	ignore = dirty
[submodule "deps/logc"]
	path = deps/logc
	url = https://github.com/realjf/logc.git
	branch = v0.1.0
	update = checkout
	ignore = dirty
[submodule "deps/SDLIMG"]
	path = deps/SDLIMG
	url = https://github.com/libsdl-org/SDL_image.git
	branch = SDL2
	update = checkout
	ignore = dirty
[submodule "deps/SDLTTF"]
	path = deps/SDLTTF
	url = https://github.com/libsdl-org/SDL_ttf.git
	branch = SDL2
	update = checkout
	ignore = dirty
[submodule "deps/spdlog"]
	path = deps/spdlog
	url = https://github.com/gabime/spdlog.git
	branch = v1.x
	update = checkout
	ignore = dirty
[submodule "deps/tinyxml"]
	path = deps/tinyxml
	url = https://github.com/realjf/tinyxml.git
	branch = master
	update = checkout
	ignore = dirty
